{
  "name": "better-auth-demo",
  "version": "1.0.0",
  "description": "Better Auth Demo with Express",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --forceExit",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "cli:generate": "npx @better-auth/cli generate --config src/infrastructure/auth.ts",
    "cli:migrate": "npx @better-auth/cli migrate --config src/infrastructure/auth.ts -y",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "auth",
    "express",
    "better-auth"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-auth": "^1.2.8",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "resend": "^4.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
